#include <stdio.h>
int main() {
    int n, i;
    printf("Enter the number of elements: ");
    scanf("%d", &n);
    if (n <= 0) {
        printf("Array size must be greater than 0.\n");
        return 1;
    }
    int nums[n];
    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
    }
    printf("First Element: %d\n", nums[0]);
    printf("Last Element: %d\n", nums[n - 1]);
    return 0;
}
